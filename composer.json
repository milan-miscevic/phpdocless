{
    "name": "milan-miscevic/phpdocless",
    "description": "@todo",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "phpdoc",
        "removal"
    ],
    "authors": [
        {
            "name": "Milan Miščević",
            "email": "milan.miscevic@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "nikic/php-parser": "^4.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Mmm\\PhpDocLess\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmm\\PhpDocLess\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
